# テスト用のTerraform変数設定ファイル
# このファイルはGitHub Actions マトリックステストで使用されます

repositories = {
  "test-repo-BRANCH_STRATEGY_PLACEHOLDER" = {
    description = "Test repository for BRANCH_STRATEGY_PLACEHOLDER strategy"
    branch_strategy = "BRANCH_STRATEGY_PLACEHOLDER"
  }
}

# ブランチ保護設定（オプション）
branch_protection_settings = {
  required_reviews           = 1
  dismiss_stale_reviews      = true
  require_code_owner_reviews = false
  enforce_admins            = false
  required_status_checks    = []
}